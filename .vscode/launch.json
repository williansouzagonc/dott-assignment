  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "args": [ "${workspaceFolder}/input.txt" ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/dist/main.js",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}